{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/PinchToScroll/bin/Debug/net8.0/PinchToScroll.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "env": {},
            "justMyCode": false,
            "sourceFileMap": {
                "/": "${workspaceFolder}"
            }
        }
    ]
}
